<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="com.cari.rbac.Role" table="SYS_RBAC_ROLE">
		<id name="role_id" column="ROLE_ID" type="string" unsaved-value="unsave">
			<generator class="assigned"></generator>
		</id>
		<property name="role_name" column="ROLE_NAME" type="string" ></property>
		<property name="role_orderNo" column="ROLE_ORDERNO" type="integer" ></property>
		<property name="role_desc" column="ROLE_DESC" type="string" ></property>
	</class>
</hibernate-mapping>
